<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asuni.imagetopdf">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:icon="@mipmap/images"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/images"
        android:theme="@style/Theme.ImageToPdf">

        <activity
            android:name=".views.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.APP_FILES" />
            </intent-filter>
        </activity>

        <activity android:name=".views.ImageToPdf"
          android:exported="false">
          <intent-filter>
            <category android:name="android.intent.category.OPENABLE"/>
          </intent-filter>
        </activity>

        <activity android:name=".views.ContactUS" />


    </application>

</manifest>
